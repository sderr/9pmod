CONFIG_9P_FS = m
CONFIG_NET_9P = m
# CONFIG_NET_9P_VIRTIO not set
CONFIG_NET_9P_RDMA = m

ccflags-y = --include $(M)/9pincludes.h \
        --include $(M)/include/net/9p/9p.h \
        --include $(M)/include/net/9p/client.h \
        --include $(M)/include/net/9p/transport.h \
        --include $(M)/include/uapi/linux/virtio_9p.h \
        --include $(M)/include/trace/events/9p.h \
	--include $(M)/9pincludes_post.h \
	-fmax-errors=2

